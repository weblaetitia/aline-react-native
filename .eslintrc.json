{
    "extends": [
        "airbnb", 
        "prettier",
        "prettier/react", 
        "prettier/prettier"
    ],
    "globals": {
      "fetch": false
    },    
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/prop-types": [0],
      "react/jsx-filename-extension": [0],
      "no-use-before-define": [0],
      "react/jsx-props-no-spreading": [0],
      "no-underscore-dangle": [0]
    }
  }